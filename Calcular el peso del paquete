payaso= float(0.112)
muñeca= float(0.075)
paquete_payaso= int(input("Por favor ingresa el total de payasos a enviar: "))
paquete_muñeca= int(input("Por favor ingresa el total de muñecas a enviar: "))
peso_payaso= float(payaso * paquete_payaso)
peso_muñeca= float(muñeca * paquete_muñeca)
total= float((payaso * paquete_payaso) + (muñeca * paquete_muñeca))
print(f"El total de payasos es: {paquete_payaso}" + " " + f"y su peso total es de: {round(peso_payaso,2)}")
print(f"El total de muñecas es: {paquete_muñeca}" + " " + f"y su peso total es de: {round(peso_muñeca,2)}")
print(f"El total del peso del paquete es: kg {round(total,2)}")
